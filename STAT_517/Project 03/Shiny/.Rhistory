geom_cor <- function(data){
data %>%
select(-ID) %>%
cor() %>%
as.data.frame() %>%
rownames_to_column() %>%
as.tibble() %>%
gather(
key = Column
,value = Correlation
,-rowname
) %>%
rename(Row = rowname) %>%
ggplot(
aes(
x = Column
,y = Row
,fill = Correlation
)
) +
geom_raster() +
scale_fill_distiller(
type = "div"
,palette = "RdBu"
,limits = c(-1, 1)
) +
theme(
axis.text.x = element_text(angle = 90, hjust = 1)
,axis.title.x = element_blank()
,axis.title.y = element_blank()
)
}
